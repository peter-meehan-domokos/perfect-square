{
    "compilerOptions": {
      "target": "ES2017",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/page.ts", "app/_components/main-content/home/page.tsx", "app/_components/visual/SVGVisual/container.js", "app/_components/perfect-square/helpers.ts", "app/_components/perfect-square/hooks_and_modules/svgComponents/perfectSquare/layout.tsx", "app/_helpers/dataHelpers.ts"],
    "exclude": ["node_modules"]
  }
  